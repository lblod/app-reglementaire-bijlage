  GRAPH ?g {

  }
}
INSERT {
  GRAPH ?g {

  }
}
WHERE {
  GRAPH ?g {
    ?snippetVersion a ext:PublishedSnippet.
    ?snippetVersion prov:derivedFrom ?derivedFrom.
  }
}