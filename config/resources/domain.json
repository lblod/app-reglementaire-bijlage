{
  "version": "0.1",
  "prefixes": {
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "nfo": "http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#",
    "dct": "http://purl.org/dc/terms/",
    "dbpedia": "http://dbpedia.org/ontology/",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#",
    "gn": "http://data.lblod.info/vocabularies/gelinktnotuleren/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "prov": "http://www.w3.org/ns/prov#",
    "pav": "http://purl.org/pav/",
    "say": "https://say.data.gift/ns/"
  },
  "resources": {
    "files": {
      "name": "file",
      "class": "nfo:FileDataObject",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "nfo:fileName"
        },
        "format": {
          "type": "string",
          "predicate": "dct:format"
        },
        "size": {
          "type": "number",
          "predicate": "nfo:fileSize"
        },
        "extension": {
          "type": "string",
          "predicate": "dbpedia:fileExtension"
        },
        "created": {
          "type": "datetime",
          "predicate": "nfo:fileCreated"
        }
      },
      "relationships": {
        "download": {
          "predicate": "nie:dataSource",
          "target": "file",
          "cardinality": "one",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://data.example.com/files/"
    },
    "templates": {
      "name": "template",
      "class": "gn:Template",
      "relationships": {
        "current-version": {
          "predicate": "pav:hasCurrentVersion",
          "target": "template-version",
          "cardinality": "one"
        },
        "versions": {
          "predicate": "pav:hasVersion",
          "target": "template-version",
          "cardinality": "many"
        },
        "derived-from": {
          "predicate": "prov:derivedFrom",
          "target": "document-container",
          "cardinality": "one"
        },
        "publisher": {
          "predicate": "dct:publisher",
          "target": "administrative-unit",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/templates/"
    },
    "template-versions": {
      "name": "template-version",
      "class": "gn:TemplateVersie",
      "super": ["file"],
      "relationships": {
        "derived-from": {
          "predicate": "prov:derivedFrom",
          "target": "editor-document",
          "cardinality": "one"
        }
      },
      "attributes": {
        "valid-through": {
          "type": "datetime",
          "predicate": "schema:validThrough"
        },
        "title": {
          "type": "string",
          "predicate": "dct:title"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/template-version/"
    },
    "regulatory-attachment-templates": {
      "name": "regulatory-attachment-template",
      "class": "gn:ReglementaireBijlageTemplate",
      "super": ["template"],
      "new-resource-base": "http://data.lblod.info/id/reglementaire-bijlage-template/"
    },
    "decision-templates": {
      "name": "decision-template",
      "class": "gn:BesluitTemplate",
      "super": ["template"],
      "new-resource-base": "http://data.lblod.info/id/besluit-template/"
    },
    "skos-concepts": {
      "name": "skos-concept",
      "class": "skos:Concept",
      "attributes": {
        "value": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "notation": {
          "type": "string",
          "predicate": "skos:notation"
        },
        "label": {
          "type": "string",
          "predicate": "ext:summary"
        },
        "search-label": {
          "type": "string",
          "predicate": "ext:searchLabel"
        },
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "in-scheme": {
          "predicate": "skos:inScheme",
          "target": "concept-scheme",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://lblod.data.gift/concepts/"
    },
    "concept-schemes": {
      "name": "concept-scheme",
      "class": "skos:ConceptScheme",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "concepts": {
          "predicate": "skos:inScheme",
          "target": "skos-concept",
          "cardinality": "many",
          "inverse": true
        },
        "top-concepts": {
          "predicate": "skos:topConceptOf",
          "target": "concept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/concept-schemes/"
    },
    "concepts": {
      "name": "concept",
      "class": "ext:Concept",
      "attributes": {
        "valid": {
          "type": "boolean",
          "predicate": "ext:valid"
        }
      },
      "new-resource-base": "http://data.lblod.info/concepts/"
    },
    "code-lists": {
      "name": "code-list",
      "class": "lblodmow:Codelist",
      "super": ["concept-scheme"],
      "attributes": {
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        }
      },
      "relationships": {
        "publisher": {
          "predicate": "dct:publisher",
          "target": "administrative-unit",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://lblod.data.gift/concept-schemes/"
    },
    "snippet-lists": {
      "name": "snippet-list",
      "class": "say:SnippetList",
      "attributes": {
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        },
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "imported-resources": {
          "type": "string-set",
          "predicate": "say:snippetImportedResource"
        }
      },
      "relationships": {
        "publisher": {
          "predicate": "dct:publisher",
          "target": "administrative-unit",
          "cardinality": "one"
        },
        "snippets": {
          "predicate": "say:hasSnippet",
          "target": "snippet",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://lblod.data.gift/snippet-lists/"
    },
    "snippets": {
      "name": "snippet",
      "class": "say:Snippet",
      "attributes": {
        "position": {
          "type": "number",
          "predicate": "schema:position"
        },
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        },
        "updated-on": {
          "type": "datetime",
          "predicate": "pav:lastUpdateOn"
        }
      },
      "relationships": {
        "current-version": {
          "predicate": "pav:hasCurrentVersion",
          "target": "snippet-version",
          "cardinality": "one"
        },
        "revisions": {
          "predicate": "pav:hasVersion",
          "target": "snippet-version",
          "cardinality": "many"
        },
        "snippet-list": {
          "predicate": "say:hasSnippet",
          "target": "snippet-list",
          "cardinality": "one",
          "inverse": true
        },
        "linked-snippet-lists": {
          "predicate": "ext:linkedSnippetList",
          "target": "snippet-list",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://data.lblod.info/id/snippets/"
    },
    "snippet-versions": {
      "name": "snippet-version",
      "class": "say:SnippetVersion",
      "relationships": {
        "previous-version": {
          "predicate": "pav:previousVersion",
          "target": "snippet-version",
          "cardinality": "one"
        },
        "snippet": {
          "predicate": "pav:hasVersion",
          "target": "snippet",
          "cardinality": "one",
          "inverse": true
        }
      },
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "content": {
          "type": "string",
          "predicate": "ext:editorDocumentContent"
        },
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        },
        "valid-through": {
          "type": "datetime",
          "predicate": "schema:validThrough"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://data.lblod.info/id/snippet-versions/"
    }
  }
}
